<form action="@Url.Action("Details","Car")">
    <label for="Id">More Info</label>
    <input id="Id" name="Id" type="number" />

    <input type="submit" class="btn btn-primary" value="Submit" />
</form>
@model CarModel.ViewModels.CarViewModel
<form action="@Url.Action("Index","Car")">
    @foreach (var car in Model.Cars)
    {
        <div>
            <p>Bil ID: @car.Id</p>
            <p>Bilmärke: @car.CarBrand</p>
            <p>RegNr: @car.RegNr</p>
            <p>Motor: @car.CarType.Enginge</p>
            <p>MotorId: @car.CarType.Id</p>
            <br />
        </div>
    }
</form>